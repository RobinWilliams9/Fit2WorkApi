@using AnvilGroup.Services.Fit2Work.Models
@model IEnumerable<UserQuestionnaireModel>
<div class="row">
    <div class="col-md-12">
        <h2>@Model.Count() Questionnaires</h2>
        <div class="clearmed"></div>
        <table class="table table-striped table-sm">
            <tr>
                <th>
                    Id
                </th>
                <th>
                    Client
                </th>
                <th>
                    Created Date
                </th>
                <th width="60%">
                    Questionnaire
                </th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.Id)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.Client.Name)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.CreatedDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item.QuestionsAndAnswersData)
                    </td>
                </tr>
            }
        </table>
    </div>
</div>
